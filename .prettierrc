{
  "semi": false,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "singleAttributePerLine": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "printWidth": 120,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "endOfLine": "lf",
  "arrowParens": "avoid",
  "proseWrap": "always"
}
